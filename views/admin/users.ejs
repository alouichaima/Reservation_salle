<!DOCTYPE html>
<head>
    <meta charset="UTF-8" />
    <title>Liste des utilisateurs</title>
    <link rel="stylesheet" href="/static/dash.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"/>
  </head>
<body>
    <div class="container">
        <%- include('../nav') -%>

        <section class="attendance">
            <div class="attendance-list">
                <h1>Users</h1>
                <table class="table">
                    <thead>
                        <tr>
                            <th>Username</th>
                            <th>Email</th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- Boucle sur les utilisateurs pour afficher leurs dÃ©tails -->
                        <% users.forEach(user => { %>
                            <tr>
                                <td><%= user.username %></td>
                                <td><%= user.email %></td>
                            </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
        </section>
</div>
</body>
</html>
